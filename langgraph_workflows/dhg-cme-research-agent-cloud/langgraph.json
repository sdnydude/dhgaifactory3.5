{
  "$schema": "https://langchain-ai.github.io/langgraph/schemas/langgraph.schema.json",
  "graphs": {
    "research_agent": {
      "path": "./src/agent.py:graph",
      "description": "Evidence-based CME research agent with multi-LLM routing (Claude, Gemini, Qwen3)"
    }
  },
  "assistants": {
    "CME Research Analyst": {
      "graph": "research_agent",
      "description": "Evidence-graded research and literature review",
      "config": {
        "configurable": {
          "output_format": "json"
        }
      }
    },
    "CME Proposal Architect": {
      "graph": "research_agent",
      "description": "Generates formal medical education activity proposals",
      "config": {
        "configurable": {
          "output_format": "cme_proposal"
        }
      }
    },
    "CME Podcast Producer": {
      "graph": "research_agent",
      "description": "Creates scripts for clinical evidence podcasts",
      "config": {
        "configurable": {
          "output_format": "podcast_script"
        }
      }
    },
    "CME Gap Analyst": {
      "graph": "research_agent",
      "description": "Focuses on identifying clinical practice gaps",
      "config": {
        "configurable": {
          "output_format": "gap_report"
        }
      }
    }
  },
  "env": ".env",
  "python_version": "3.11",
  "dependencies": [
    "langgraph>=0.2.0",
    "langchain>=0.3.0",
    "langchain-anthropic>=0.2.0",
    "langchain-google-genai>=2.0.0",
    "langchain-community>=0.3.0",
    "langsmith>=0.1.140",
    "httpx>=0.27.0"
  ]
}
